            <Group name="Lights">
                <GlobalIllumination name="GlobalLight">
                    <ambient r=".25" g=".25" b=".25"/>
                </GlobalIllumination>
                <PointLight name="theLight" range="3" parent="">
                    <ambient r="0.25" g="0.25" b="0.25"/>
                    <diffuse r="1" g="1" b="1"/>
                    <specular r="1" g="1" b="1"/>
                    <position x="0" y="1" z="0"/>
                </PointLight>
            </Group>
            <Group name="Effects"/>
