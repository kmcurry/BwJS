<Session broadcast="false">
	<Set target="Selector" computePivotDistance="false"/>
	<MouseHandler>
		<name>MouseHandler</name>
		<numResponses>-1</numResponses>
	</MouseHandler>
	<ObjectInspector>
		<name>ObjectInspector</name>
	</ObjectInspector>
	<BwSceneInspector>
		<name>SceneInspector</name>
		<viewRelativeXAxisRotation>true</viewRelativeXAxisRotation>
		<viewRelativeZAxisRotation>true</viewRelativeZAxisRotation>
		<pivotDistance>3</pivotDistance>
		<rotationSensitivity item0="0.5" item1="0.5" item2="0.5"/>
	</BwSceneInspector>
	<Isolator>
		<name>Root</name>
		<isolateTransforms>true</isolateTransforms>
		<PerspectiveCamera>
			<name>Camera</name>
			<position item0="3" item1="4.2424" item2="-3"/>
			<rotation item0="45" item1="-45" item2="0"/>
			<nearDistance>0.25</nearDistance>
			<farDistance>4000</farDistance>
			<zoom>3.2</zoom>
		</PerspectiveCamera>
		<Group>
			<name>Lights</name>
			<GlobalIllumination>
				<name>GlobalLight</name>
				<ambient item0="0.25" item1="0.25" item2="0.25" item3="0.25"/>
			</GlobalIllumination>
			<DirectionalLight>
				<name>theLight</name>
				<enabled>true</enabled>
				<rotation item0="0" item1="0" item2="0"/>
				<ambient item0="0.25" item1="0.25" item2="0.25" item3="1"/>
				<diffuse item0="1" item1="1" item2="1" item3="1"/>
				<specular item0="1" item1="1" item2="1" item3="1"/>
			</DirectionalLight>
		</Group>
		<Group>
			<name>Effects</name>
		</Group>
		<Group>
			<name>Models</name>
			<ModelRoot>
				<Model>
					<name>grid</name>
					<position item0="0" item1="0" item2="0"/>
					<rotation item0="0" item1="0" item2="0"/>
					<scale item0="1" item1="1" item2="1"/>
					<url>/objects/GridG.lwo</url>
				</Model>
			</ModelRoot>
			<ModelRoot>
				<Model>
					<name>UV_BoxG</name>
					<position item0="0" item1="0.26" item2="0"/>
					<rotation item0="0" item1="0" item2="0"/>
					<scale item0="1" item1="1" item2="1"/>
					<url>/objects/UV_BoxG.lwo</url>
					<indexedGeometry>false</indexedGeometry>
				</Model>
			</ModelRoot>
			<ModelRoot>
				<Model>
					<name>cylinderRight</name>
					<position item0="2" item1="0.26" item2="0"/>
					<rotation item0="0" item1="0" item2="0"/>
					<scale item0="1" item1="1" item2="1"/>
					<url>/objects/UV_CylinderG.lwo</url>
				</Model>
			</ModelRoot>
			<ModelRoot>
				<Model>
					<name>cylinderLeft</name>
					<position item0="-2" item1="0.26" item2="0"/>
					<rotation item0="0" item1="0" item2="0"/>
					<scale item0="1" item1="1" item2="1"/>
					<url>/objects/UV_CylinderG.lwo</url>
					<enableSharing>false</enableSharing>
				</Model>
			</ModelRoot>
			<ModelRoot>
				<Model>
					<name>sphereBack</name>
					<position item0="0" item1="0.25" item2="2"/>
					<rotation item0="0" item1="0" item2="0"/>
					<scale item0="1" item1="1" item2="1"/>
					<url>/objects/UV_SphereG.lwo</url>
				</Model>
			</ModelRoot>
			<ModelRoot>
				<Model>
					<name>sphereFront</name>
					<position item0="0" item1="0.25" item2="-2"/>
					<rotation item0="0" item1="0" item2="0"/>
					<scale item0="1" item1="1" item2="1"/>
					<url>/objects/UV_SphereG.lwo</url>
					<enableSharing>false</enableSharing>
				</Model>
			</ModelRoot>
			<ModelRoot>
				<Model>
					<name>F16</name>
					<url>/objects/F16.lwo</url>
				</Model>
			</ModelRoot>
		</Group>
		<KeyframeInterpolator>
			<name>F16Motion1</name>
			<url>motions/F16New.mot</url>
			<target>F16</target>
		</KeyframeInterpolator>
	</Isolator>
	<RenderDirective>
		<name>RenderDirective</name>
		<backgroundImageFilename>http://localhost/bwjs/demo/default/BwContent/images/Sky2.jpg</backgroundImageFilename>
		<root>Root</root>
	</RenderDirective>
	<SerializeDirective>
		<root>Root</root>
	</SerializeDirective>
	<SetCommand>
		<name>SetCapture_Down_Rot_Scene</name>
		<numResponses>-1</numResponses>
		<event>VK_DOWN.Down</event>
		<target>SceneInspector</target>
		<pivotDistance>3</pivotDistance>
	</SetCommand>
	<SetCommand>
		<name>SetCapture_Up_Rot_Scene</name>
		<numResponses>-1</numResponses>
		<event>VK_UP.Down</event>
		<target>SceneInspector</target>
		<pivotDistance>3</pivotDistance>
	</SetCommand>
	<SetCommand>
		<name>SetCapture_Left_Rot_Scene</name>
		<numResponses>-1</numResponses>
		<event>VK_LEFT.Down</event>
		<target>SceneInspector</target>
		<pivotDistance>3</pivotDistance>
	</SetCommand>
	<SetCommand>
		<name>SetCapture_Right_Rot_Scene</name>
		<numResponses>-1</numResponses>
		<event>VK_RIGHT.Down</event>
		<target>SceneInspector</target>
		<pivotDistance>3</pivotDistance>
	</SetCommand>
	<SetCommand>
		<name>SetCapture_Left_Pan_Scene</name>
		<numResponses>-1</numResponses>
		<event>VK_CONTROL+VK_LEFT.Down</event>
		<target>SceneInspector</target>
		<pivotDistance>3</pivotDistance>
	</SetCommand>
	<SetCommand>
		<name>SetCapture_Right_Pan_Scene</name>
		<numResponses>-1</numResponses>
		<event>VK_CONTROL+VK_RIGHT.Down</event>
		<target>SceneInspector</target>
		<pivotDistance>3</pivotDistance>
	</SetCommand>
	<SetCommand>
		<name>SetCapture_Up_Pan_Scene</name>
		<numResponses>-1</numResponses>
		<event>VK_CONTROL+VK_UP.Down</event>
		<target>SceneInspector</target>
		<pivotDistance>3</pivotDistance>
	</SetCommand>
	<SetCommand>
		<name>SetCapture_Down_Pan_Scene</name>
		<numResponses>-1</numResponses>
		<event>VK_CONTROL+VK_DOWN.Down</event>
		<target>SceneInspector</target>
		<pivotDistance>3</pivotDistance>
	</SetCommand>
	<SetCommand>
		<name>SetCapture_ZoomIn_Scene</name>
		<numResponses>-1</numResponses>
		<event>VK_PLUS.Down</event>
		<target>SceneInspector</target>
		<pivotDistance>3</pivotDistance>
	</SetCommand>
	<SetCommand>
		<name>SetCapture_ZoomOut_Scene</name>
		<numResponses>-1</numResponses>
		<event>VK_MINUS.Down</event>
		<target>SceneInspector</target>
		<pivotDistance>3</pivotDistance>
	</SetCommand>
	<SetCommand>
		<name>SetCapture_Down_Rot_Objects</name>
		<numResponses>-1</numResponses>
		<event>VK_DOWN.Down</event>
		<target>ObjectInspector</target>
	</SetCommand>
	<SetCommand>
		<name>SetCapture_Up_Rot_Objects</name>
		<numResponses>-1</numResponses>
		<event>VK_UP.Down</event>
		<target>ObjectInspector</target>
	</SetCommand>
	<SetCommand>
		<name>SetCapture_Left_Rot_Objects</name>
		<numResponses>-1</numResponses>
		<event>VK_LEFT.Down</event>
		<target>ObjectInspector</target>
	</SetCommand>
	<SetCommand>
		<name>SetCapture_Right_Rot_Objects</name>
		<numResponses>-1</numResponses>
		<event>VK_RIGHT.Down</event>
		<target>ObjectInspector</target>
	</SetCommand>
	<SetCommand>
		<name>SetCapture_S_Pan_Objects</name>
		<numResponses>-1</numResponses>
		<event>VK_CONTROL+VK_LEFT.Down</event>
		<target>ObjectInspector</target>
	</SetCommand>
	<SetCommand>
		<name>SetCapture_F_Pan_Objects</name>
		<numResponses>-1</numResponses>
		<event>VK_CONTROL+VK_RIGHT.Down</event>
		<target>ObjectInspector</target>
	</SetCommand>
	<SetCommand>
		<name>SetCapture_C_Pan_Objects</name>
		<numResponses>-1</numResponses>
		<event>VK_CONTROL+VK_UP.Down</event>
		<target>ObjectInspector</target>
	</SetCommand>
	<SetCommand>
		<name>SetCapture_V_Pan_Objects</name>
		<numResponses>-1</numResponses>
		<event>VK_CONTROL+VK_DOWN.Down</event>
		<target>ObjectInspector</target>
	</SetCommand>
	<SetCommand>
		<name>SetCapture_E_Pan_Objects</name>
		<numResponses>-1</numResponses>
		<event>VK_MINUS.Down</event>
		<target>ObjectInspector</target>
	</SetCommand>
	<SetCommand>
		<name>SetCapture_D_Pan_Objects</name>
		<numResponses>-1</numResponses>
		<event>VK_PLUS.Down</event>
		<target>ObjectInspector</target>
	</SetCommand>
	<PlayCommand>
		<numResponses>-1</numResponses>
		<event>VK_PERIOD</event>
	</PlayCommand>
	<CommandSequence/>
	<CommandSequence/>
	<CommandSequence/>
	<CommandSequence/>
	<CommandSequence/>
	<CommandSequence/>
	<SetCommand>
		<numResponses>-1</numResponses>
		<event>VK_ADD</event>
		<target>RenderAgent</target>
	</SetCommand>
	<SetCommand>
		<numResponses>-1</numResponses>
		<event>VK_SUBTRACT</event>
		<target>RenderAgent</target>
	</SetCommand>
	<Serialize>
		<target>Root</target>
	</Serialize>
</Session>